{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "prefer-for-of": false,
        "no-var-requires": false,
        "ordered-imports": false,
        "no-trailing-whitespace": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false
    },
    "rulesDirectory": []
}